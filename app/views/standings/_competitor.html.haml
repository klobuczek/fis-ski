%tr{:style => "font-weight: #{competitor.qualified? ? 'bold' : 'normal'};"}
  %td{:align => :right}
    =competitor.rank
  %td{:align => :right}
    =competitor.fis_code
  %td
    %a{:href => competitor.href}
      =competitor.name
  %td
    =competitor.year
  %td{:align => :right}
    =competitor.nation
  %td{:align => :right}
    =competitor.cup_points
  -if tie_breaker  
    %td{:align => :right}
      -if competitor.tie
        ='%.2f' % competitor.fis_points
  %td
    %table
      %tr
        =render :partial => 'result', :collection => competitor.results, :locals => {:tie => (competitor.tie and tie_breaker)}